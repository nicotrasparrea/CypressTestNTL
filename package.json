{
  "name": "cyframework",
  "version": "1.0.0",
  "description": "A test automation framework template using Cypress + BDD + POM + Reporter + Dashboard",
  "scripts": {
    "cy:dev:chrome": "cypress run --config baseUrl=http://devtodo.monfared.io/ --browser chrome --record",
    "cy:stag:chrome": "cypress run --config baseUrl=http://stagtodo.monfared.io/ --browser chrome --record",
    "cy:dev:firefox": "cypress run --config baseUrl=http://devtodo.monfared.io/ --browser firefox --record",
    "cy:stag:firefox": "cypress run --config baseUrl=http://stagtodo.monfared.io/ --browser firefox --record",
    "cy:headed": "cypress run --config baseUrl=http://devtodo.monfared.io/ --headed",
    "cy:test": "cypress run --config baseUrl=http://devtodo.monfared.io/",
    "reports:clear": "if exist cypress\\videos rmdir /q /s cypress\\videos && if exist cypress\\screenshots rmdir /q /s cypress\\screenshots && if exist cypress\\reports rmdir /q /s cypress\\reports",
    "allure:clear": "if exist allure-report rmdir /q /s allure-report && if exist allure-results rmdir /q /s allure-results && if exist cypress\\screenshots rmdir /q /s cypress\\screenshots && if exist cypress\\videos rmdir /q /s cypress\\videos && if exist reports rmdir /q /s reports",
    "cy:run": "npx cypress run --env allure=true,allureReuseAfterSpec=true --browser chrome",
    "cy:run_qa": "cypress run --env allure=true --env configFile=qa --browser chrome",
    "cy:report": "node ./cucumber-html-report.js",
    "allure:report": "allure generate allure-results --clean -o allure-report"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mmonfared/CyFramework.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/mmonfared/CyFramework/issues"
  },
  "homepage": "https://github.com/mmonfared/CyFramework#readme",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^16.0.3",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.0",
    "cypress": "^10.7.0",
    "cypress-xpath": "^2.0.1",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.37",
    "multiple-cucumber-html-reporter": "^2.0.0",
    "cypress-on-fix": "^1.0.2"
  },
  "cypress-cucumber-preprocessor": {
    "step_definitions": "cypress/support/step_definitions/",
    "nonGlobalStepDefinitions": false
  },
  "main": "cypress.config.js",
  "dependencies": {
    "@shelex/cypress-allure-plugin": "^2.40.0",
    "cucumber-json-report-formatter": "^0.1.4",
    "multiple-cucumber-html-reporter": "^3.4.0"
  }
}
